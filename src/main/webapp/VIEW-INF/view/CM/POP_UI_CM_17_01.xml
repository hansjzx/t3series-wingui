    <component id="POP_UI_CM_17_01_CONFIRM" type="BUTTON">
        <props>
            <name>EXEC</name>
            <lang>true</lang>
        </props>
        <actions>
            <action event-type="click">
                <operation-call id="OPC_POP_UI_CM_17_01_CLOSE">
                    <component-id>POP_UI_CM_17_01</component-id>
                    <operation-id>CLOSE</operation-id>
                    <conditions>
                        <condition id="POP_UI_CM_17_01_EXEC_COND">
                            <component>POP_UI_CM_17_01_11_02</component>
                            <operator>notEqual</operator>
                            <value>empty</value>
                            <msg>MSG_5114</msg>
                        </condition>
                    </conditions>
                    <success>
	                    <operation-call id="OPC_WAIT_ON">
	                        <component-id>COMMON</component-id>
	                        <operation-id>WAIT-ON</operation-id>
	                    </operation-call>
                        <operation-call id="OPC_POP_UI_CM_17_01_CONFIRM_SAVE">
                            <component-id>POP_UI_CM_17_01_CONFIRM</component-id>
                            <operation-id>SAVE</operation-id>
                            <parameters>
                                <parameter id="SIMUL_VER_ID" reference-id="POP_UI_CM_17_01_11_02" default-value="" />
                                <parameter id="SIMUL_VER_DESCRIP" reference-id="POP_UI_CM_17_01_12_02" default-value="" />
                                <parameter id="PLAN_SNRIO_MGMT_DTL_ID" reference-id="RST_CPT_01:selections" extract-by="PLAN_SNRIO_MGMT_DTL_ID" default-value="" />
                                <parameter id="USER_ID" reference-id="COMMON:loginId" default-value="" />
                            </parameters>
                            <conditions>
                                <condition id="CHECK_RUN_COND_02" group="A">
                                    <component>RST_CPT_01</component>
                                    <column>PROCESS_TP</column>
                                    <operator>equal</operator>
                                    <value>SPROC_03</value>
                                </condition>
                                <condition id="CHECK_RUN_COND_04" group="B">
                                    <component>RST_CPT_01</component>
                                    <column>PROCESS_TP</column>
                                    <operator>equal</operator>
                                    <value>SPROC_05</value>
                                </condition>
                            </conditions>
                            <success>
                                <operation-call id="OPC_RST_CPT_02_RUN_PLAN">
                                    <component-id>POP_UI_CM_17_01_CONFIRM</component-id>
                                    <operation-id>RUN_PLAN</operation-id>
                                    <success>
                                        <operation-call id="OPC_POP_UI_CM_17_01_SUCCESS_MSG_01">
                                            <component-id>RST_CPT_01</component-id>
                                            <operation-id>LOAD</operation-id>
                                        </operation-call>
                                        <operation-call id="OPC_POP_UI_CM_17_01_SUCCESS_MSG_02">
                                            <component-id>COMMON</component-id>
                                            <operation-id>MESSAGE</operation-id>
                                            <parameters>
                                                <parameter id="msg" value="MSG_0003" />
                                            </parameters>
                                        </operation-call>
                                    </success>
                                </operation-call>
                            </success>
                            <complete>
                                <operation-call id="OPC_WAIT_OFF">
                                    <component-id>COMMON</component-id>
                                    <operation-id>WAIT-OFF</operation-id>
                                </operation-call>
                            </complete>
                        </operation-call>
                        <operation-call id="OPC_RST_CPT_02_CONFIRM">
                            <component-id>POP_UI_CM_17_01_CONFIRM</component-id>
                            <operation-id>CONFIRM</operation-id>
                            <conditions>
                                <condition id="CHECK_CNFRM_COND_01" group="A">
                                    <component>RST_CPT_01</component>
                                    <column>PROCESS_TP</column>
                                    <operator>equal</operator>
                                    <value>SPROC_04</value>
                                </condition>
                                <condition id="CHECK_CNFRM_COND_02" group="B">
                                    <component>RST_CPT_01</component>
                                    <column>PROCESS_TP</column>
                                    <operator>equal</operator>
                                    <value>SPROC_06</value>
                                </condition>
                            </conditions>
                            <success>
                                <operation-call id="OPC_POP_UI_CM_17_01_SUCCESS_MSG_03">
                                    <component-id>COMMON</component-id>
                                    <operation-id>MESSAGE</operation-id>
                                    <parameters>
                                        <parameter id="msg" value="MSG_0003" />
                                    </parameters>
                                </operation-call>
                            </success>
                         <complete>
                             <operation-call id="OPC_WAIT_OFF">
                                 <component-id>COMMON</component-id>
                                 <operation-id>WAIT-OFF</operation-id>
                             </operation-call>
                         </complete>
                        </operation-call>
                    </success>
                </operation-call>
            </action>
        </actions>
        <operations>
            <operation id="SAVE">
                <service-call id="SVC_UI_CM_17_S4_CONFIRM_SAVE">
                    <service-id>SRV_UI_CM_17_S4</service-id>
                    <service-target>T3SeriesSupplyNetServer</service-target>
                </service-call>
            </operation>
            <operation id="CONFIRM">
                <service-call id="service-call-confirm-plan">
                    <service-id>ConfirmPlan</service-id>
                    <service-target>T3SeriesSupplyNetServer</service-target>
                    <parameters>
                        <parameter id="VERSION_ID" reference-id="POP_UI_CM_17_01_11_02" default-value="" />
                        <parameter id="USER_ID" reference-id="COMMON:loginId" />
                    </parameters>
                </service-call>
            </operation>
            <operation id="RUN_PLAN">
                <service-call id="service-call-run-plan">
                    <service-id>RunPlan</service-id>
                    <service-target>T3SeriesSupplyNetServer</service-target>
                    <parameters>
                        <parameter id="PLAN_POLICY_MGMT_ID" reference-id="RST_CPT_01:selections" extract-by="PLAN_POLICY_MGMT_ID" default-value="" />
                        <parameter id="VERSION_ID" reference-id="POP_UI_CM_17_01_11_02" default-value="" />
                    </parameters>
                </service-call>
            </operation>
            <operation id="RUN_BATCH_PLAN">
                <service-call id="service-call-run-batch-plan">
                    <service-id>RunBatchPlan</service-id>
                    <service-target>T3SeriesSupplyNetServer</service-target>
                    <parameters>
                        <parameter id="PLAN_POLICY_MGMT_ID" reference-id="RST_CPT_01:selections" extract-by="PLAN_POLICY_MGMT_ID" default-value="" />
                        <parameter id="VERSION_ID" reference-id="POP_UI_CM_17_01_11_02" default-value="" />
                    </parameters>
                </service-call>
            </operation>
            <operation id="RESTART_SERVER">
                <service-call id="service-call-restart">
                    <service-id>RestartServer</service-id>
                    <service-target>T3SeriesSupplyNetServer</service-target>
                </service-call>
            </operation>
        </operations>
    </component>
